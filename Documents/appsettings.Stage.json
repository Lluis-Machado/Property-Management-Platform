{
  "Auth0": {
    "BaseUrl": "https://stage-auth0.example.com",
    "Audience": "wuf-app"
  },
  "AzureBlobStorage": {
    "StorageAccount": "plattesstage"
  },
  "ConnectionStrings": {
    "SqlConnection": "server=(LocalDb)\\MSSQLLocalDB; database=Documents; Integrated Security=true; Encrypt=false"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://localhost:27017/logs",
          "collectionName": "logs",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000",
          "rollingInterval": "Month"
        }
      }
    ]
  }
}